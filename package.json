{
  "name": "expo-needs-update",
  "version": "1.0.0",
  "description": "A React hook for checking if your Expo app needs to be updated by comparing with App Store and Play Store versions",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "expo",
    "react-native",
    "react-hook",
    "update",
    "version",
    "check",
    "app-store",
    "play-store",
    "semver"
  ],
  "author": "Phillip Lakis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/philliplakis/expo-needs-update.git"
  },
  "bugs": {
    "url": "https://github.com/philliplakis/expo-needs-update/issues"
  },
  "homepage": "https://github.com/philliplakis/expo-needs-update#readme",
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "expo": ">=49.0.0",
    "expo-application": ">=5.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "expo-application": "^5.0.0",
    "jest": "^29.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "^0.72.0",
    "react-test-renderer": "^18.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
